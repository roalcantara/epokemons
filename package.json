{
  "name": "poc",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "clear:cache": "rm -rf node_modules/.cache/nx && jest --clearCache",
    "clear": "rm -rf dist tmp coverage && yarn clear:cache",
    "purge": "yarn clear && rm -rf node_modules",
    "reset": "yarn purge && yarn install",
    "reset:force": "yarn purge && rm -rf ./yarn.lock && yarn",
    "build:all": "nx run-many --target=build --all",
    "test:all": "nx run-many --target=test --all --parallel --runInBand",
    "e2e:all": "nx run-many --target=e2e --all",
    "lint:all": "nx run-many --target=lint --all --parallel",
    "lint:time": "TIMING=1 nx lint"
  },
  "engines": {
    "node": "^14.x",
    "yarn": ">=1.22.4 <2"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^7.6.11",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.11",
    "@nestjs/platform-express": "^7.6.11",
    "@nestjs/terminus": "^7.1.0",
    "cross-fetch": "^3.0.6",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.3",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.11",
    "@nrwl/cli": "11.2.10",
    "@nrwl/eslint-plugin-nx": "11.2.10",
    "@nrwl/jest": "^11.2.10",
    "@nrwl/linter": "11.2.10",
    "@nrwl/nest": "^11.2.10",
    "@nrwl/node": "^11.2.10",
    "@nrwl/tao": "11.2.10",
    "@nrwl/workspace": "11.2.10",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.25",
    "@types/pg": "^7.14.9",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "dotenv": "8.2.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
