{
  "name": "poc",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "clear:cache": "rm -rf node_modules/.cache/nx && jest --clearCache",
    "clear": "rm -rf dist tmp coverage && yarn clear:cache",
    "purge": "yarn clear && rm -rf node_modules",
    "reset": "yarn purge && yarn install",
    "reset:force": "yarn purge && rm -rf ./yarn.lock && yarn",
    "build:all": "nx run-many --target=build --all",
    "test:all": "nx run-many --target=test --all --parallel --runInBand",
    "e2e:all": "nx run-many --target=e2e --all",
    "lint:all": "nx run-many --target=lint --all --parallel",
    "lint:time": "TIMING=1 nx lint"
  },
  "engines": {
    "node": "^14.x",
    "yarn": ">=1.22.4 <2"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@nrwl/cli": "11.2.10",
    "@nrwl/tao": "11.2.10",
    "@nrwl/workspace": "11.2.10",
    "@types/node": "14.14.25",
    "dotenv": "8.2.0",
    "eslint": "7.19.0",
    "prettier": "2.2.1",
    "ts-node": "~9.1.1",
    "typescript": "~4.1.3"
  }
}
